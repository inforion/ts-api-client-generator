{
  "name": "ts-api-client-generator",
  "version": "1.0.1",
  "description": "Generates type-aware REST API client from config written with Typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/inforion/ts-api-client-generator.git"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js"
  },
  "keywords": [
    "Typescript",
    "REST",
    "API",
    "HTTP",
    "generation"
  ],
  "main": "./dist/bundle.js",
  "types": "./src/index.d.ts",
  "files": [
    "dist",
    "src/**/*.ts"
  ],
  "author": "Nikolay Kudryavtsev <n.kudriavtsev@inforion.ru>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/node": "14.11.2",
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-disable": "2.0.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-unicorn": "19.0.1",
    "ts-loader": "8.0.4",
    "typescript": "4.0.2",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12"
  }
}