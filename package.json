{
  "name": "ts-api-client-generator",
  "version": "1.0.4",
  "description": "Generates type-aware REST API client from config written with Typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/inforion/ts-api-client-generator.git"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js"
  },
  "keywords": [
    "Typescript",
    "REST",
